package upei.database.mock;

import java.time.LocalDate;
import java.util.LinkedList;
import java.util.List;

import upei.domain.*;

public class SprintsMock {
	private List<Sprint> sprints;
	
	public SprintsMock() {
		sprints = new LinkedList<Sprint>();
		
		sprints.add(new Sprint("Sprint 05/01", LocalDate.now()));
		sprints.add(new Sprint("Sprint 05/02", LocalDate.now()));
		sprints.add(new Sprint("Sprint 05/03", LocalDate.now()));
	}
	
	public List<Sprint> obterSprints(){
		return this.sprints;
	}
}
