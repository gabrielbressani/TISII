public class SprintsAPI extends BaseAPI {
	
	public SprintsAPI(Request request, Response response) {
		super(request, response);
	}

	public void obterUltimaSprintAtiva() {
		Sprint ultimaSprint = new Sprint("Sprint 05/02");
		
		this.returnJson();
	}
}